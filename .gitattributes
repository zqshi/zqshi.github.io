# Auto detect text files and perform LF normalization
* text=auto

# Explicitly declare text files you want to always be normalized and converted to native line endings on checkout
*.py text eol=lf
*.md text eol=lf
*.txt text eol=lf
*.json text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
*.xml text eol=lf
*.html text eol=lf
*.css text eol=lf
*.js text eol=lf
*.ts text eol=lf
*.sh text eol=lf
*.dockerfile text eol=lf
Dockerfile text eol=lf

# Declare files that will always have CRLF line endings on checkout
*.bat text eol=crlf
*.cmd text eol=crlf

# Denote all files that are truly binary and should not be modified
*.png binary
*.jpg binary
*.jpeg binary
*.gif binary
*.ico binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.flv binary
*.fla binary
*.swf binary
*.gz binary
*.zip binary
*.7z binary
*.ttf binary
*.eot binary
*.woff binary
*.woff2 binary
*.pyc binary
*.pdf binary
*.ez binary
*.bz2 binary
*.swp binary
*.jar binary

# Archive files
*.7z binary
*.jar binary
*.rar binary
*.zip binary
*.gz binary
*.bz2 binary
*.xz binary
*.tar binary

# Python compiled files
*.pyc binary
*.pyo binary
*.pyd binary

# AI/ML model files
*.model binary
*.pkl binary
*.joblib binary
*.h5 binary
*.hdf5 binary

# Database files
*.db binary
*.sqlite binary
*.sqlite3 binary

# Exclude from exports
.gitattributes export-ignore
.gitignore export-ignore
.claude/ export-ignore
test_* export-ignore
*_test.py export-ignore